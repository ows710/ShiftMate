<html xmlns="http://www.w3.org/1999/xhtml" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:w2="http://www.inswave.com/websquare" xmlns:xf="http://www.w3.org/2002/xforms">
        
    <head meta_screenName="근무자 관리 페이지">

    	<w2:type>COMPONENT</w2:type>

    	<w2:buildDate />

    	<w2:MSA />

    	<xf:model>

    		<w2:dataCollection baseNode="map">
    			<w2:dataList baseNode="list" repeatNode="map" id="dlt_dutyinfo" saveRemovedData="true">
    				<w2:columnInfo>
    					<w2:column dataType="text" name="이름" id="name"></w2:column>
    					<w2:column dataType="text" name="직위" id="position"></w2:column>
    					<w2:column dataType="date" name="입사일" id="hire_date"></w2:column>
    					<w2:column dataType="text" name="상태" id="nurse_status"></w2:column>
    					<w2:column dataType="text" name="부서" id="dept_name"></w2:column>
    				</w2:columnInfo>
    			</w2:dataList>
    		</w2:dataCollection>

    		<w2:workflowCollection />

    	</xf:model>

    	<w2:layoutInfo />

    	<w2:publicInfo method="" />

    	<script lazy="false" type="text/javascript"><![CDATA[
scwin.onpageload = function() {

};

scwin.btn_import_onclick = function (e) {
    var jsnOptions;

    jsnOptions = {
        headerExist: "1", //[default: 0] Excel 파일에서 그리드의 데이터에 header가 있는지 여부(1이면 header 존재 0이면 없음)
        trim: 1 //[default: 0, 1] 엑셀 각각의 셀데이터 좌, 우에 공백이 있는 경우 제거할지 여부 (1이면 제거, 0이면 유지)
    };

    //GridView "grd"의 엑셀 업로드 실행
    grd.advancedExcelUpload(jsnOptions);
};

scwin.btn_export_onclick = function (e) {
    // 옵션 변수
    var options = {};
    // xlsx, xls, csv 구분 ex) .csv
 
    // 옵션 설정
	options.fileName = "근무자관리.xlsx";
    
    // 다운로드
    grd.advancedExcelDownload(options);
};]]></script>
    	<w2:require as="udc_notification" type="page" variableClone="" src="/InsWebApp/cm/udc/matrix/notification.xml"></w2:require>
    </head>

    <body class="" ev:onpageload="scwin.onpageload">

    	<xf:group class="sub_contents" id="" meta_snippetCategory="00_화면시작" meta_snippetKeyComponent="true" meta_snippetName="0_01 페이지시작"
    		style="">
    		<xf:group class="titbox" id="" meta_snippetCategory="02_타이틀" meta_snippetKeyComponent="true" meta_snippetName="2_02 서브타이틀(h3)"
    			style="">
    			<w2:textbox class="" id="" label="근무자 관리" style="" tagname="h3"></w2:textbox>
    			<xf:group class="rt" id="" style="">
    				<xf:trigger class="btn_cm  row_add" id="" style="" type="button">
    					<xf:label><![CDATA[행추가]]></xf:label>
    				</xf:trigger>
    				<xf:trigger class="btn_cm  row_del" id="" style="" type="button">
    					<xf:label><![CDATA[행삭제]]></xf:label>
    				</xf:trigger>
    				<xf:trigger class="btn_cm  upload" id="btn_import" style="" type="button" ev:onclick="scwin.btn_import_onclick">
    					<xf:label><![CDATA[엑셀 가져오기]]></xf:label>
    				</xf:trigger>
    				<xf:trigger ev:onclick="scwin.btn_export_onclick" style="" id="btn_export" type="button" class="btn_cm  download">
    					<xf:label><![CDATA[엑셀 내보내기]]></xf:label>
    				</xf:trigger>
    				<xf:input meta_snippetCategory="10_입력폼" meta_snippetName="10_10 InputBox_조회버튼" meta_snippetKeyComponent="true"
    					style="width:150px;" id="" placeholder="" class="">
    				</xf:input>
    				<xf:trigger meta_snippetCategory="10_입력폼" meta_snippetName="10_10 InputBox_조회버튼" style="" id="" type="button"
    					class="btn_cm search icon">
    					<xf:label><![CDATA[Search]]></xf:label>
    				</xf:trigger>
    			</xf:group>

    		</xf:group>
    		<xf:group adaptiveThreshold="" class="gvwbox" id="" meta_snippetCategory="06_그리드" meta_snippetKeyComponent="true"
    			meta_snippetName="6_02 그리드_순번_상태" style="">
    		</xf:group>
    		<xf:group adaptiveThreshold="" class="gvwbox" id="" meta_snippetCategory="06_그리드" meta_snippetKeyComponent="true"
    			meta_snippetName="6_01 그리드" style="">
    			<xf:group class="pglbox" id="" meta_snippetCategory="06_그리드" meta_snippetKeyComponent="true"
    				meta_snippetName="6_03 페이지리스트">
    			</xf:group>
    		</xf:group>
    		<xf:group adaptiveThreshold="" class="gvwbox" id="" meta_snippetCategory="06_그리드" meta_snippetKeyComponent="true"
    			meta_snippetName="6_02 그리드_순번_상태" style="">
    			<w2:gridView autoFit="allColumn" checkReadOnlyOnPasteEnable="" class="gvw" dataList="data:dlt_dutyinfo" focusMode="row"
    				id="grd" keepDefaultColumnWidth="true" rowNumHeaderValue="No" rowNumVisible="true" rowNumWidth="50" rowStatusHeaderValue="상태"
    				rowStatusVisible="true" rowStatusWidth="50" scrollByColumn="false" scrollByColumnAdaptive="false" style="height: 150px;"
    				visibleRowNum="30">
    				<w2:caption id="caption4" style="" value="this is a grid caption."></w2:caption>
    				<w2:header id="header2" style="">
    					<w2:row id="row3" style="">
    						<w2:column blockSelect="false" displayMode="label" id="column11" inputType="text" removeBorderStyle="false"
    							value="이름" width="70">
    						</w2:column>
    						<w2:column blockSelect="false" displayMode="label" id="column10" inputType="text" removeBorderStyle="false"
    							value="직위" width="70">
    						</w2:column>
    						<w2:column blockSelect="false" displayMode="label" id="column9" inputType="text" removeBorderStyle="false"
    							value="입사일" width="70">
    						</w2:column>
    						<w2:column blockSelect="false" displayMode="label" id="column8" inputType="text" removeBorderStyle="false"
    							value="상태" width="70">
    						</w2:column>
    						<w2:column blockSelect="false" displayMode="label" id="column7" inputType="text" removeBorderStyle="false"
    							value="부서" width="70">
    						</w2:column>
    					</w2:row>
    				</w2:header>
    				<w2:gBody id="gBody2" style="">
    					<w2:row id="row4" style="">
    						<w2:column blockSelect="false" displayMode="label" id="name" inputType="text" removeBorderStyle="false"
    							width="70">
    						</w2:column>
    						<w2:column blockSelect="false" displayMode="label" id="position" inputType="text" removeBorderStyle="false"
    							width="70">
    						</w2:column>
    						<w2:column blockSelect="false" displayMode="label" id="hire_date" inputType="text" removeBorderStyle="false"
    							width="70">
    						</w2:column>
    						<w2:column blockSelect="false" displayMode="label" id="nurse_status" inputType="text" removeBorderStyle="false"
    							width="70">
    						</w2:column>
    						<w2:column blockSelect="false" displayMode="label" id="dept_name" inputType="text" removeBorderStyle="false"
    							width="70">
    						</w2:column>
    					</w2:row>
    				</w2:gBody>
    			</w2:gridView>
    		</xf:group>
    		<w2:udc_notification style="width: 30px;height: 30px;" title="" message=""></w2:udc_notification>
    	</xf:group>
    </body>

</html>
